
<div class="row">
    <div class="col-8 offset-2">
        <div class="mb-5">
            <select id="optionFile" class="form-control">
                <option value="1">JSON</option>
                <option value="2">PIPES</option>
            </select>
        </div>
        <div class="mb-5">
            <button class="btn btn-primary" onclick="generate();">Generar</button>
        </div>
    </div>
</div>

<script>
    function generate() 
    {
        let optionFile = document.getElementById("optionFile").value;
        fetch("@Url.Content("~/GeneratorFile/CreateFile")"+"?optionFile="+optionFile)
            .then(res => res.json())
            .then(json => alert(json));
    }
</script>